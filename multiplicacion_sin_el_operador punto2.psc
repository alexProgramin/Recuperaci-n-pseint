Proceso multiplicacion_sin_el_operador
	
	definir numero, numero2 ,multiplicacion,i como entero;
	
	numero <- 2;
	numero2 <- 3;
	multiplicacion <- 0;
	
	Para i <- 1 Hasta numero2  Hacer
		multiplicacion <- multiplicacion + numero;
	FinPara
	escribir"la multiplicacion es: ", multiplicacion;
	
FinProceso
